{
  "name": "react-basics",
  "version": "1.0.0",
  "description": "Project focusing on using reactJS",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server",
    "serv:data": "./node_modules/.bin/babel-node ./server/dataServ.js --port 3333 --delay 1000",
    "serv:src": "./node_modules/.bin/babel-node ./server/srcServer.js",
    "serv:dist": "./node_modules/.bin/babel-node ./server/distServer.js",
    "test": "mocha --reporter spec src/tests/testSetup.js src/**/*.test.js src/tests/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "./node_modules/.bin/rimraf ./dist",
    "build:js": "babel-node build/build.js",
    "build:html": "babel-node build/buildHtml.js",
    "build": "npm-run-all clean-dist test build:js build:html"
  },
  "author": "Mike Borowy",
  "license": "ISC",
